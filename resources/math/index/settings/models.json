{
    "zeta": {
        "context": "2d",
        "view": {
            "figsize": [
                10,
                10
            ],
            "frameon": false
        },
        "params": {
            "font.family": {
                "sans-serif": [
                    "Helvetica"
                ]
            },
            "text.usetex": true
        },
        "axes": {},
        "tick_params": {
            "labelcolor": "white", "labelsize": 12
        },
        "facecolor": "black",
        "spines": {
            "left": {
                "color": "red",
                "position": "zero",
                "linewidth": 1.0
            },
            "right": {
                "color": "red",
                "position": "zero",
                "linewidth": 1.0
            },
            "bottom": {
                "color": "red",
                "position": "zero",
                "linewidth": 1.0
            },
            "top": {
                "color": "red",
                "position": "zero",
                "linewidth": 1.0
            }
        },
        "plot": {
            "line": {
                "lw": 1, "color": "white"
            },
            "point": {
                "marker": "o",
                "markersize": 16,
                "color": "white"
            }
        },
        "axis": {
            "x": {"label": "x","position": "bottom"},
            "y": {"label": "y","position": "left"},
            "style": {
                "pad": 5,
                "labelsize": 10
            }
        },
        "title": {
            "text": "Re vs. Im",
            "style": {"color": "black"}
        },
        "subtitle": {
                "text": "\\mathcal{T}(\\mathcal{C}ircuital\\otimes\\mathcal{M}inds)",
                "style": {"fontsize":18, "color": "teal", "fontfamily": "sans-serif"},
                "position": [0.7, 0.15]
        },
        "label": {
            "axis": ["x", "y"],
            "style": {"fontsize": 18, "color": "teal"}
        },
        "colors": ["#ffffff", "#009797", "#99d5d5", "#4cb6b6"]
    },
    "mandelbrot": {
        "title": "\\mathcal{C} ircuital \\hspace{1mm} \\mathcal{M} inds.",
        "imshow": {"interpolation": "hamming", "cmap": "Spectral_r"}
    }
}